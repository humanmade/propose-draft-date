    "@wordpress/date": "^3.9.0",
    "@wordpress/edit-post": "^3.18.0",
    "@wordpress/i18n": "^3.13.0",
    "@wordpress/jest-preset-default": "^6.1.0",
    "block-editor-hmr": "^0.6.0",
    "eslint": "^6.6.0",
    "eslint-config-humanmade": "^0.8.0",
    "eslint-config-react-app": "^3.0.8",
