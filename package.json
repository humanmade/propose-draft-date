  "license": "GPL-2.0-or-later",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "ie >= 11"
  ],
  "scripts": {
    "build": "webpack --config=.config/webpack.config.prod.js",
    "start": "webpack-dev-server --config=.config/webpack.config.dev.js",
    "lint:js": "eslint --cache-location=.cache/eslint.json --ext js,jsx,ts,tsx .eslintrc.js .config src",
    "lint:js:fix": "eslint --fix --cache-location=.cache/eslint.json --ext js,jsx,ts,tsx .eslintrc.js .config src",
    "lint:php": "composer run-script lint",
    "lint:php:fix": "composer run-script lint:fix",
    "lint": "npm-run-all lint:*",
    "test:php": "docker run --rm -v \"$PWD:/code\" humanmade/plugin-tester",
    "test:js": "jest",
    "test:js:watch": "jest --watch",
    "test:js:coverage": "jest --coverage",
    "test": "npm run lint && npm-run-all test:*"
  },
  "devDependencies": {
    "@humanmade/webpack-helpers": "^0.7.1",
